<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="48dp"
    android:height="48dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- Q circle with gradient -->
    <path
        android:pathData="M50,10 C72,10 90,28 90,50 C90,72 72,90 50,90 C28,90 10,72 10,50 C10,28 28,10 50,10 Z M50,25 C36,25 25,36 25,50 C25,64 36,75 50,75 C64,75 75,64 75,50 C75,36 64,25 50,25 Z"
        android:fillType="evenOdd">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:startX="50"
                android:startY="10"
                android:endX="50"
                android:endY="90">
                <item android:offset="0.0" android:color="#FFFF10F0"/>
                <item android:offset="0.3" android:color="#FFEC4899"/>
                <item android:offset="0.5" android:color="#FFC19EE0"/>
                <item android:offset="0.7" android:color="#FF9D4EDD"/>
                <item android:offset="1.0" android:color="#FF6D28D9"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Q tail -->
    <path
        android:pathData="M60,65 L85,95 L72,95 L52,72 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:startX="52"
                android:startY="65"
                android:endX="85"
                android:endY="95">
                <item android:offset="0.0" android:color="#FF9D4EDD"/>
                <item android:offset="1.0" android:color="#FF6D28D9"/>
            </gradient>
        </aapt:attr>
    </path>
</vector>
