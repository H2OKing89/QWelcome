<?xml version="1.0" encoding="utf-8"?>
<!--
   Data extraction rules for QWelcome app (Android 12+).
   Excludes sensitive DataStore data from cloud backup and device transfer.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude DataStore file containing sensitive settings (tech profile, templates) -->
        <exclude domain="file" path="datastore/settings.preferences_pb" />
        <exclude domain="sharedpref" path="settings.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude DataStore from device-to-device transfers as well -->
        <exclude domain="file" path="datastore/settings.preferences_pb" />
        <exclude domain="sharedpref" path="settings.preferences_pb" />
    </device-transfer>
</data-extraction-rules>