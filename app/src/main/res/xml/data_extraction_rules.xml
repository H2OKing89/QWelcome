<?xml version="1.0" encoding="utf-8"?>
<!--
   Data extraction rules for QWelcome app (Android 12+).
   Excludes sensitive DataStore data from cloud backup and device transfer.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude entire DataStore directory (contains sensitive settings) -->
        <exclude domain="file" path="datastore/" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude entire DataStore directory from device-to-device transfers -->
        <exclude domain="file" path="datastore/" />
    </device-transfer>
</data-extraction-rules>