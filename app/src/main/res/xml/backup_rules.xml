<?xml version="1.0" encoding="utf-8"?>
<!--
   Backup rules for QWelcome app (Android 11 and below).
   Excludes sensitive data from auto-backup.
   See https://developer.android.com/guide/topics/data/autobackup
-->
<full-backup-content>
    <!-- Exclude entire DataStore directory (contains sensitive settings) -->
    <exclude domain="file" path="datastore/" />
</full-backup-content>

